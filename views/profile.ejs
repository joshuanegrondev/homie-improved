                </div>
                <a href="/post-item" class="btn btn-outline-white border-width-2 d-none d-lg-inline-block"><span class="mr-2 icon-add"></span>Donate An Item</a>
                <!-- goes app.get(/post-item) on routes.js  -->
              </div>
              <div class="row mb-5">
                <div class="col-12 col-sm-6 col-md-6 col-lg-3 mb-4 mb-lg-0">
                </div>
                <div class="col-12 col-sm-6 col-md-6 col-lg-3 mb-4 mb-lg-0">
                  <a href="/jobs-listings" class="btn btn-primary btn-lg btn-block text-white btn-search">View All Jobs</a>
                </div>
